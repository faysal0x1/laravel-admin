{
    "name": "syntorit/laravel-admin",
    "description": "Laravel admin panel package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Faysal Rahman",
            "email": "faysal.swe6@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "intervention/image": "^2.7",
        "laravel/breeze": "^2.1",
        "php-flasher/flasher-laravel": "^2.0",
        "santigarcor/laratrust": "^8.3",
        "yajra/laravel-datatables-oracle": "^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAdmin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAdmin\\Providers\\LaravelAdminServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}